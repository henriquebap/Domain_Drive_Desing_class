package aula_methods;

public class testeConta {
	public static void main(String[] args) {
		conta c1 = new conta();
		c1.verficSaldo();
		System.out.println("Titular: " + c1.getTitula());
		System.out.println("Agencia" + c1.getAgencia());
		System.out.println("Conta: " + c1.getnConta());
		
		c1.setTitula("Eduardo");
		c1.setAgencia("123-123");
		c1.setnConta("4400");
		c1.setSaldo(1000);
		
		c1.verficSaldo();
		System.out.println("Titular: " + c1.getTitula());
		System.out.println("Agencia" + c1.getAgencia());
		System.out.println("Conta: " + c1.getnConta());
		
		
	}
}
