package aula_methods;

import java.util.Scanner;

public class testeConta {
	public static void main(String[] args) {
		Scanner input = new Scanner(System.in);
		
		conta c1 = new conta();
		conta c2 = new conta();
		conta c3 = new conta();
		
		
		
		
		c1.verficSaldo();
		System.out.println("Escreva o nome do Titular: " );
		String nome_1 = input.nextLine();
		c1.setTitula(nome_1);
		
		System.out.println("Digite a Agencia " );
		
		
		System.out.println("Conta: " + c1.getnConta());
		
		c1.setTitula("Eduardo");
		c1.setAgencia("123-123");
		c1.setnConta("4400");
		c1.setSaldo(1000);
		
		System.out.println("");
		
		c1.verficSaldo();
		System.out.println("Titular: " + c1.getTitula());
		System.out.println("Agencia: " + c1.getAgencia());
		System.out.println("Conta: " + c1.getnConta());
		
		
	}
}
